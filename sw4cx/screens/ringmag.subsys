    MAGX_IST_CDAC20_LINE(d3m1t3, `"3M1-3"',  "ιπ1: ι-χ-1000-12",  1000, 1, icd_3m1, 0, 0)
    MAGX_IST_CDAC20_LINE(d4m1t3, `"4M1-3"',  "ιπ1: ι-χ-1000-13",  1000, 1, icd_4m1, 0, 0)
    MAGX_IST_CDAC20_LINE(d5m1t4, `"5M1-4"',  "ιπ1: ιστ-13",       1000, 1, icd_i13, 0, 1) # rev
    MAGX_IST_CDAC20_LINE(d6m1t4, `"6M1-4"',  "ιπ1: ιστ-14",       1000, 1, icd_i14, 0, 1) # rev
    MAGX_MPS20_CEAC124_LINE(d6m5n6, `"6M5,6"', "ιπ1: ςστ5",   13,   1, rst5_mps1)
    MAGX_MPS20_CEAC124_LINE(d6m7n8, `"6M7,8"', "ιπ1: ςστ5",   13,   1, rst5_mps2)
