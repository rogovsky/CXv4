    MAGX_IST_CDAC20_LINE(qd2,    `"D2"',     "ιπ1: ιστ-8",        1000, 1, icd_d2,  0, 0)
    MAGX_IST_CDAC20_LINE(qd3,    `"D3"',     "ιπ1: ιστ-7",        1000, 1, icd_d3,  0, 0)
    MAGX_IST_CDAC20_LINE(qf3,    `"F3"',     "ιπ1: ιστ-10",       1000, 1, icd_f3,  0, 0)
    RINGMAG_SECTION("χΩΠΥΣΛ")
    MAGX_IST_CDAC20_LINE(d3m4n5, `"3M4,5"',  "ιπ1: ι-χ-1000-10",  1000, 1, icd_3m4, 0, 0)
